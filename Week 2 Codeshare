CLASSES
class Dog{
//properties
int age;
color fur;
int size;
string breed;

//constructor 
Dog(){
	age = 1;
  size = 10;
  
}

//methods
void bark();
void jump();
void kill();
}

//new instance

Dog chester = new Dog();
